{
    "name": "dialmycalls/php-sdk-v2",
    "description": "API client for the DialMyCalls.com API",
    "keywords": ["api", "rest", "dialmycalls", "sms", "call"],
    "homepage": "http://www.dialmycalls.com",
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "autoload": {
        "psr-4": { "DialMyCalls\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "DialMyCalls\\" : "test/" }
    }
}

